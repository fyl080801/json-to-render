{
  "fields": [
    {
      "component": "p",
      "condition": "$:model.obj.selected",
      "props": {
        "innerText": { "$type": "computed", "$result": "model.text1" }
      }
    },
    {
      "component": "input",
      "props": {
        "value": { "$type": "computed", "$result": "model.text1" },
        "onInput": {
          "$type": "method",
          "$context": "model.text1",
          "$result": "arguments[0].target.value"
        }
      }
    },
    {
      "component": "input",
      "context": "model.text1"
    },
    {
      "component": "el-input",
      "context": "model.text1"
    },
    {
      "component": "el-select",
      "context": "model.obj.selected",
      "children": [
        {
          "component": "el-option",
          "props": { "value": 0, "label": "选项1" }
        },
        {
          "component": "el-option",
          "props": { "value": 1, "label": "选项2" }
        }
      ]
    },
    {
      "component": "span",
      "condition": "$:model.obj.selected",
      "text": "$:model.text1"
    }
  ]
}
